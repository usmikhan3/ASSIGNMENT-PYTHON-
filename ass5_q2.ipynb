    "def lowup(s):\n",
    "    low = 0\n",
    "    up = 0\n",
    "    for i in s:\n",
    "        if(i.islower()):\n",
    "            low=low+1\n",
    "        else:\n",
    "            up=up+1\n",
    "    print(\"LOWERCASE LETTERS ARE \"  )\n",
    "    print(low)\n",
    "    print(\"\\nUPPERCASE LETTERS ARE \"  )\n",
    "    print(up)\n",
    "lowup('USmaN')\n",

    "def test_prime(n):\n",
    "    if (n==1):\n",
    "        return \"not a prime number\"\n",
    "    elif (n==2):\n",
    "        return \"it is a prime number\";\n",
    "    else:\n",
    "        for x in range(2,n):\n",
    "            if(n % x==0):\n",
    "                return False\n",
    "        return True             \n",
    "print(test_prime(11))\n"
   ]
